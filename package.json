{
  "name": "WebLinhTinh",
  "version": "0.0.1",
  "description": "Monorepo for NestJS backend and Vue.js frontend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "be",
    "fe"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:be\" \"npm run dev:fe\"",
    "dev:be": "npm run start:dev --workspace=be",
    "dev:fe": "npm run dev --workspace=fe",
    "build": "npm run build --workspaces",
    "build:be": "npm run build --workspace=be",
    "build:fe": "npm run build --workspace=fe"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
